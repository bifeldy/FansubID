{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "resources": {
        "files": [
          "/*.(ico|svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ],
        "urls": [
          "/assets/**",
          "https://i.ibb.co/**",
          "https://fonts.gstatic.com/**",
          "https://i.mydramalist.com/**",
          "https://cdn.myanimelist.net/**",
          "https://fonts.googleapis.com/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api",
      "urls": [
        "/api/aktivasi/**",
        "/api/anime/**",
        "/api/anime-berkas/**",
        "/api/anime-fansub/**",
        "/api/anime-seasonal/**",
        "/api/api-key/**",
        "/api/banned/**",
        "/api/berkas/**",
        "/api/comment/**",
        "/api/dorama/**",
        "/api/dorama-berkas/**",
        "/api/dorama-fansub/**",
        "/api/dorama-seasonal/**",
        "/api/fansub/**",
        "/api/fansub-all/**",
        "/api/fansub-anime/**",
        "/api/fansub-berkas/**",
        "/api/fansub-dorama/**",
        "/api/fansub-rss-feed/**",
        "/api/fansub-slug/**",
        "/api/image/**",
        "/api/likedislike/**",
        "/api/login/**",
        "/api/logout/**",
        "/api/news/**",
        "/api/nihongo/**",
        "/api/nihongo-edict/**",
        "/api/nihongo-hirakata/**",
        "/api/nihongo-hirakata-all/**",
        "/api/nihongo-kanji/**",
        "/api/nihongo-kanjivg/**",
        "/api/nihongo-lesson/**",
        "/api/nihongo-tatoeba/**",
        "/api/notification/**",
        "/api/project-type/**",
        "/api/promote/**",
        "/api/register/**",
        "/api/torrent/**",
        "/api/user/**",
        "/api/verify/**",
        "/api/verify-ktp/**",
        "/api/verify-nik/**",
        "/api/verify-sosmed/**"
      ],
      "cacheConfig": {
        "maxSize": 64,
        "maxAge": "1d",
        "timeout": "0u",
        "strategy": "freshness"
      }
    }
  ]
}
